{
	"options":{
		"override":false,
		"update":false
	},
	"attributes":{
		"title":"Example Page",
		"id":"example",
		"path":"example.php",
		"minifyOptions":{
			"css":{
				"name":"exampleCSS",
				"folder":"min/example"
			}
		},
		"root":"front/ioframe/",
		"definitionsRoot":"templates/",
		"definitionsFile":"definitions.php",
		"templatesRootVar":"$IOFrameTemplateRoot",
		"cssRootVar":"$IOFrameCSSRoot",
		"jsRootVar":"$IOFrameJSRoot",
		"templatesRoot":"templates/",
		"cssRoot":"css/",
		"jsRoot":"js/",
		"cssHandler":"$CSSResources",
		"jsHandler":"$JSResources",
		"cssArray":"$CSS",
		"jsArray":"$JS",
		"templates":{
			"headersStartTemplate":"headers_start.php",
			"headersGetResourcesTemplate":"headers_get_resources.php",
			"headersEndTemplate":"headers_end.php",
			"footersStartTemplate":"footers_start.php",
			"footersEndTemplate": "footers_end.php"
		}
	},
	"template":"pages/base.txt",
	"variables":{
		"HAS_WRAPPER":true,
		"EXAMPLE_CONSOLE_LOG":"This is an example!",
		"IS_CONTROL_PANEL":true
	},
	"items":{
		"js":[
			"mixins/sourceURL.js",
			{
				"path":"mixins/eventHubManager.js",
				"create":false,
				"pageLocation":"headers"
			},
			{
				"path":"components/searchList.js",
				"create":false,
				"handler":"$JSResources",
				"resourceArray":"$JS",
				"rootVar":"$IOFrameJSRoot"
			},
			{
				"path":"components/example/exampleEditor.js",
				"create":true,
				"template":"js/components/viewUpdateCreateComponent.txt",
				"variables":{
					"COMPONENT_NAME":"example-editor",
					"MIXINS":"sourceURL,eventHubManager",
					"EXAMPLE_PROPS":true,
					"EXAMPLE_DATA":false,
					"EXAMPLE_METHODS":true,
					"EXAMPLE_CREATED":true
				}
			},
			{
				"path":"modules/CPMenu.js",
				"create":false
			},
			{
				"path":"modules/example.js",
				"create":true,
				"template":"js/modules/searchListAppCustomSet.txt",
				"variables":{
					"VARIABLE_NAME":"exampleEditor",
					"ELEMENT_NAME":"example-editor",
					"VUE_NAME":"Example Editor",
					"MIXINS":"sourceURL,eventHubManager",
					"EXAMPLE_DATA":true,
					"EXAMPLE_METHODS":true,
					"EXAMPLE_CREATED":false
				}
			}
		],
		"css":[
			{
				"path":"animations.css",
				"create":false,
				"pageLocation":"footers"
			},
			{
				"path":"components/searchList.css",
				"create":false
			},
			{
				"path":"components/example/exampleViewer.css",
				"create":true
			},
			{
				"path":"modules/CPMenu.css",
				"create":false
			},
			{
				"path":"modules/example.css",
				"create":true
			},
			{
				"path":"https://www.example.com/example.css",
				"local":false,
				"pageLocation":"footers"
			}
		],
		"templates":[
			{
				"path":"modules/CPMenu.php",
				"create":false
			},
			{
				"path":"modules/example.php",
				"create":true
			}
		]
	}
}