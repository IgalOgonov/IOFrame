{
	"options":{
		"override":false,
		"update":false
	},
	"attributes":{
		"title":"Settings",
		"id":"settings",
		"path":"cp/settings.php"
	},
	"template":"pages/base.txt",
	"variables":{
		"HAS_WRAPPER":true,
		"IS_CONTROL_PANEL":true
	},
	"items":{
		"js":[
			"mixins/sourceURL.js",
			"mixins/eventHubManager.js",
			"components/searchList.js",
			{
				"path":"components/settings/settingsEditor.js",
				"create":true,
				"template":"js/components/viewUpdateCreateComponent.txt",
				"variables":{
					"COMPONENT_NAME":"settings-editor",
					"MIXINS":"sourceURL,eventHubManager,IOFrameCommons"
				}
			},
			"modules/CPMenu.js",
			{
				"path":"modules/settings.js",
				"create":true,
				"template":"js/modules/searchListAppCustomSet.txt",
				"variables":{
					"VARIABLE_NAME":"settings",
					"ELEMENT_NAME":"settings",
					"VUE_NAME":"settings",
					"MIXINS":"sourceURL,eventHubManager,IOFrameCommons"
				}
			}
		],
		"css":[
				"cp.css",
				"components/searchList.css",
				{
					"path":"components/settings/settingsEditor.css",
					"create":true
				},
				{
					"path":"modules/settings.css",
					"create":true
				},
				"modules/CPMenu.css"
		],
		"templates":[
			{
				"path":"modules/CPMenu.php",
				"create":false
			},
			{
				"path":"modules/settings.php",
				"create":true,
				"template":"templates/modules/baseAppModule.txt",
				"variables":{
					"ELEMENT_NAME":"settings",
					"COMPONENT_NAME":"settings-editor"
				}
			}
		]
	}
}